{
  "name": "trans-comm-ui",
  "version": "0.0.0",
  "workspaces": {
    "nohoist": [
      "**/cypress/**",
      "**/cypress",
      "**/@cypress",
      "**/@cypress/**"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --host 0.0.0.0 --poll 1",
    "build": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "e2e": "ng e2e",
    "test": "ng run TransCommUi:test-jest --colors",
    "test:watch": "ng run TransCommUi:test-jest --colors --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "source-map-explorer": "source-map-explorer dist/transcomm-ui/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "^27.2.1",
    "@ag-grid-community/client-side-row-model": "^27.2.1",
    "@ag-grid-community/core": "^27.2.1",
    "@angular/animations": "~13.3.1",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "~13.3.1",
    "@angular/forms": "~13.3.1",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/router": "~13.3.1",
    "axios": "^0.21.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular/cli": "~13.3.1",
    "@angular/compiler-cli": "~13.3.1",
    "@cypress/schematic": "1.6.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^15.6.1",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.0.1",
    "karma-coverage": "~2.0.3",
    "msw": "^0.36.8",
    "ts-jest": "^27.1.2",
    "typescript": "~4.6.3",
    "cypress": "latest"
  }
}
